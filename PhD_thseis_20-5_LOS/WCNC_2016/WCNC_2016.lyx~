	filename tc_measured_2.emf
	lyxscale 12
	width 100col%

\end_inset


\end_layout

\begin_layout Plain Layout
\begin_inset Caption Standard

\begin_layout Plain Layout
Collided slot timing
\begin_inset CommandInset label
LatexCommand label
name "fig:Collided-Slot-Duration"

\end_inset


\end_layout

\end_inset


\end_layout

\end_inset


\end_layout

